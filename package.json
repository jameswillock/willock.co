{
  "name": "willock.co",
  "version": "1.0.0",
  "description": "Curriculum vitae for James Willock",
  "main": "index.ts",
  "scripts": {
    "test": "mocha --reporter spec --require ts-node/register --require jsdom-global/register ./ts/*.test.ts",
    "deploy": "gh-pages -d dist",
    "dev": "parcel index.pug",
    "build": "rm -rf dist/* && parcel build index.pug --public-url ./ && cp CNAME dist/CNAME && cp keybase.txt dist/keybase.txt"
  },
