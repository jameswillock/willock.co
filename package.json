  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jameswillock/willock.co/issues"
  },
  "homepage": "https://github.com/jameswillock/willock.co/issues#readme",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "eslint": "^7.2.0",
    "gh-pages": "^3.0.0",
    "jsdom": "16.2.2",
    "jsdom-global": "3.0.2",
    "mocha": "^8.0.1",
    "normalize.css": "^8.0.1",
    "parcel-bundler": "^1.12.4",
    "pug": "^3.0.0",
    "sass": "^1.26.8",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
